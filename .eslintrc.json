{
  "extends": ["eslint-config-brave"],
  "rules": {
    "import/no-unresolved": ["error", { "ignore": ["@env"] }],
    "react/prop-types": "off",
    "react/jsx-one-expression-per-line": "off"
  },
  "overrides": [
    {
      "files": ["src/redux/slices/*"],
      "rules": {
        "no-param-reassign": "off"
      }
    }
  ]
}
